<div class="@IconWindowName">
    <div class="icon-grid">
            @foreach (var iconClass in Icons)
            {
                 <Icon IconClass="@iconClass" IconColor="@IconColor" OnIconSelected="OnIconSelected" IsEventSet=true />
            }
    </div>
</div>

@code {
    [Parameter] 
    public EventCallback<string> OnIconSelected { get; set; }
    [Parameter]
    public string IconWindowName { get; set; }
    [Parameter]
    public string IconColor { get; set; }
    private List<string> Icons = IconList.Icons;
}
